@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sdo:   <https://schema.org/> .


schema:IndividualProductShape
    a sh:NodeShape ;
    sh:targetClass schema:IndividualProduct;
    sh:Property[
sh:predicate sdo:id;
sh:minCount 1;
sh:maxCount 1;
sh:dataType sh:IRI;

] ;
sh:Property[
sh:predicate sdo:name;
sh:minCount 1;
sh:maxCount 1;
sh:dataType xsd:string;

] ;
sh:Property[
sh:predicate sdo:version;
sh:minCount 1;
sh:maxCount 1;
sh:dataType xsd:string;

] ;
 
sh:Property[
sh:predicate sdo:schemaVersion;
sh:minCount 1;
sh:maxCount 1;
sh:dataType sh:IRI;

] ;
sh:Property[
sh:predicate sdo:serialNumber;
sh:minCount 1;
sh:maxCount 1;
sh:dataType xsd:decimal;

] .




